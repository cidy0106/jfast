<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<filter>
		<filter-name>jfast</filter-name>
		<filter-class>com.xidige.jfast.web.DefaultFilter</filter-class>
		<init-param>
			<param-name>jf.param.init.configfactory</param-name>
			<param-value>com.xidige.jfastdemo.MyConfigLoadFactory</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>jfast</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
</web-app>
